{
  "name": "sgh-schedule",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "crawlee": "tsx src/utils/main.ts",
    "dev": "vite --open",
    "generate:manifest-icons": "pwa-asset-generator public/app-icon.png public/icons --favicon -i ./index.html",
    "generate:readme": "npx marked -o readme.html -i README.md",
    "lint": "eslint src --ext ts,tsx",
    "prebuild": "npm run crawlee",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "vite",
    "test": "playwright test -c playwright.config.ts",
    "test:regenerate": "playwright test -c playwright.config.ts --update-snapshots",
    "tsc": "tsc",
    "weekly": "playwright test -c playwright.weekly.config.ts --project=\"Mobile Chrome\" -u"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.38.1",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.78",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "commander": "^12.0.0",
    "crawlee": "^3.9.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "9.0.11",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-pkg": "0.18.1",
    "prettier-plugin-sh": "^0.14.0",
    "pwa-asset-generator": "^6.3.1",
    "replace-in-file": "^7.1.0",
    "tailwindcss": "^3.4.3",
    "tsx": "^4.7.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vite-plugin-pwa": "^0.19.8"
  }
}
