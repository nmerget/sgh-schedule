{
  "name": "sgh-schedule",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "crawlee": "tsx src/utils/main.ts",
    "dev": "vite --open",
    "generate:manifest-icons": "pwa-asset-generator public/app-icon.png public/icons --favicon -i ./index.html",
    "generate:readme": "npx marked -o readme.html -i README.md",
    "lint": "eslint src --ext ts,tsx",
    "prebuild": "npm run crawlee",
    "prepare": "husky",
    "preview": "vite preview",
    "start": "vite",
    "test": "playwright test -c playwright.config.ts",
    "test:regenerate": "playwright test -c playwright.config.ts --update-snapshots",
    "tsc": "tsc",
    "weekly": "playwright test -c playwright.weekly.config.ts --project=\"Mobile Chrome\" -u"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.38.1",
    "@types/node": "^22.10.2",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "commander": "^12.1.0",
    "crawlee": "^3.12.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "husky": "9.1.7",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-pkg": "0.18.1",
    "prettier-plugin-sh": "^0.14.0",
    "pwa-asset-generator": "^6.4.0",
    "replace-in-file": "^8.2.0",
    "tailwindcss": "^4.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^6.0.3",
    "vite-plugin-pwa": "^0.21.1"
  }
}
